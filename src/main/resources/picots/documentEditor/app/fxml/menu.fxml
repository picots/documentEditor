<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/23.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="app.controller.MenuController">
	<menus>
		<Menu mnemonicParsing="false" text="File">
			<items>
				<MenuItem mnemonicParsing="false" text="Import" />
				<MenuItem mnemonicParsing="false" text="Export" />
				<SeparatorMenuItem mnemonicParsing="false" />
				<MenuItem mnemonicParsing="false" text="Close" />
			</items>
		</Menu>
		<Menu mnemonicParsing="false" text="Document">
			<items>
				<MenuItem mnemonicParsing="false" text="new HTML document" onAction="#onNewHTML"/>
				<MenuItem mnemonicParsing="false" text="new MarkDown document" onAction="#onNewMarkDown"/>
			</items>
		</Menu>
		<Menu mnemonicParsing="false" text="Help">
			<items>
				<MenuItem mnemonicParsing="false" text="About" />
			</items>
		</Menu>
	</menus>
</MenuBar>